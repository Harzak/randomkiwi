{
  "App": {
    "Name": "randomkiwi",
    "UserPreferencesFile": "userPreferences.json",
    "BookmarksFile": "bookmarks.json",
    "ScriptNamespace": "Scripts",
    "ScriptUIFormattingFilename": "wikipedia-article-ui-formatting"
  },
  "UserMetrics": {
    "MaxRecentNavigations": 20,
    "BaseSizePool": 20
  },
  "ArticleCatalog": {
    "PoolThreshold": 20,
    "CatalogThreshold": 20
  },
  "HttpClient": {
    "DefaultClientName": "randomwiki"
  },
  "Wikipedia": {
    "UrlFormat": "https://{0}.m.wikipedia.org",
    "QueryEndpointFormat": "/w/api.php?action=query&generator=random&grnnamespace={0}&grnlimit={1}&prop=pageprops&grnfilterredir=nonredirects&prop=pageprops%7Cinfo&inprop=length&format=json"
  }
}